{
    "venvPath": ".",
    "venv": "venv",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.14",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "./venv/lib/python3.14/site-packages"
            ]
        }
    ],
    "include": [
        "**/*.py"
    ],
    "exclude": [
        "**/__pycache__",
        "**/node_modules"
    ],
    "reportMissingImports": true,
    "reportMissingTypeStubs": false
}

